<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sinsokeyt.spring.test.mybatis.repository.RealEstateRepository01">
 	<select id="selectRealEstate01" resultType="com.sinsokeyt.spring.test.mybatis.domain.RealEstate"
 	parameterType="int">
 		select id
			,realtorId
			,address
			,area
			,type
			,price
			,rentPrice
			,createdAt
			,updatedAt
		from real_estate
		where id=#{id};
 	</select>
 </mapper>