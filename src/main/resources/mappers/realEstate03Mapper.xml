<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sinsokeyt.spring.test.mybatis.repository.RealEstateRepository03">
 	<select id="selectRealEstate03" parameterType="int" resultType="com.sinsokeyt.spring.test.mybatis.domain.RealEstate">
 		select id
			,realtorId
			,address
			,area
			,type
			,price
			,rentPrice
			,createdAt
			,updatedAt
		from real_estate
		where area <![CDATA[ >= ]]> #{area} and price <![CDATA[ <= ]]> #{price};
 	</select>
 </mapper>