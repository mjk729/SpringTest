<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sinsokeyt.spring.test.database.repository.StoreRepository">
 	<select id="selectStoreList" parameterType="int" resultType="com.sinsokeyt.spring.test.database.domain.Store">
 		select
 			id
			,name
			,phoneNumber
			,address
			,businessNumber
			,introduce
			,createdAt
			,updatedAt
		from 
			store;
 	</select>
 </mapper>