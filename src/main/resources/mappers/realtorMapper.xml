<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.sinsokeyt.spring.test.jsp.repository.RealtorRepository">
 
 	<insert id="insertRealtor" parameterType="com.sinsokeyt.spring.test.jsp.domain.Realtor" 
 	useGeneratedKeys="true" keyProperty="id">
 	insert into 
 		realtor
 	(
 		office
 		,phoneNumber
 		,address
 		,grade	
 	)
 	value
 	(
 		#{office}
 		,#{phoneNumber}
 		,#{address}
 		,#{grade}
 	)
 	
 	</insert>
 	
 </mapper>